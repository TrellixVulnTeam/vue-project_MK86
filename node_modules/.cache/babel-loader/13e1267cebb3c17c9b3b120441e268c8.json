t-panel\" @submit.prevent=\"createNewTwoot\" :class=\"{'--exceeded': newTwootCharacterCount > 180}\">\r\n        <label for=\"newTwoot\"><strong>New Twoot</strong> ({{newTwootCharacterCount}}/180) </label>\r\n        <textarea id=\"newTwoot\" rows=\"4\" v-model=\"state.newTwootContent\"/>\r\n\r\n        <div class=\"create-twoot-panel_submit\">\r\n            <div class=\"create-twoot-type\">\r\n                <label for=\"newTwootType\"><strong>Type: </strong></label>\r\n                <select id=\"newTwootType\" v-model=\"state.selectedTwootType\">\r\n                    <option :value=\"option.value\" v-for=\"(option, index) in state.twootTypes\" :key=\"index\">\r\n                        {{ option.name }}\r\n                    </option>\r\n                </select>\r\n            </div>\r\n        \r\n\r\n            <button>Twoot It!</button>\r\n        </div>\r\n    </form>\r\n</template>\r\n\r\n<script>\r\nimport { reactive, computed } from 'vue';\r\nexport default {\r\n    name: \"CreateTwootPanel\",\r\n\r\n    setup(props, ctx) {\r\n      const state = reactive({\r\n        newTwootContent: '',\r\n        selectedTwootType: 'instant',\r\n        twootTypes: [\r\n            {value: 'draft', name: 'Draft'},\r\n            {value: 'instant', name: 'Instant Twoot'}\r\n        ]\r\n      })\r\n\r\n      const newTwootCharacterCount = computed( () => state.newTwootContent.length )\r\n\r\n      function createNewTwoot(){\r\n        if (state.newTwootContent && state.selectedTwootType !== 'draft'){\r\n            ctx.emit('add-twoot', state.newTwootContent);\r\n            state.newTwootContent = '';\r\n        }\r\n      }\r\n      \r\n      return {\r\n        state,\r\n        newTwootCharacterCount,\r\n        createNewTwoot\r\n      }\r\n    }\r\n     \r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.create-twoot-panel {\r\n  margin-top: 20px;\r\n  padding: 20px 0;\r\n  display: flex;\r\n  flex-direction: column;\r\n  textarea {\r\n    border: 1px solid #DFE3E8;\r\n    border-radius: 5px;\r\n  }\r\n  .create-twoot-panel_submit {\r\n    display: flex;\r\n    justify-content: space-between;\r\n    .create-twoot-type {\r\n      padding: 10px 0;\r\n    }\r\n    button {\r\n      padding: 5px 20px;\r\n      margin: auto 0;\r\n      border-radius: 5px;\r\n      border: none;\r\n      background-color: teal;\r\n      color: white;\r\n      font-weight: bold;\r\n    }\r\n  }\r\n  &.--exceeded {\r\n    color: red;\r\n    border-color: red;\r\n    .create-twoot-panel_submit {\r\n      button {\r\n        background-color: red;\r\n        color: white;\r\n      }\r\n    }\r\n  }\r\n}\r\n</style>"],"sourceRoot":""}]}